<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test Automation Suite">
    <test name="Include Regression Defect group test">
        <groups>
            <run>
                <include name="regression" />
                <exclude name="defect.*" />
            </run>
        </groups>
        <classes>
            <class name="annotations.AmazonFakeSite"></class>
        </classes>
    </test>
</suite>